A = int(input())
B = int(input())
C = int(input())
D = A+B+C
if(D<=49):
    print("F")
elif(D<=54):
    print("D")
elif(D<=59):
    print("D+")
elif(D<=64):
    print("C")
elif(D<=69):
    print("C+")
elif(D<=74):
    print("B")
elif(D<=79):
    print("B+")
elif(D<=100):
    print("A")
else:
    print("error")